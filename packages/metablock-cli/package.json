{
  "name": "@metablock/cli",
  "version": "0.2.8",
  "description": "A node client for interacting with metablock cloud",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/quantmind/metablock-js.git",
    "directory": "packages/metablock-cli"
  },
  "author": "Quantmind",
  "license": "ISC",
  "dependencies": {
    "@metablock/core": "0.2.8",
    "archiver": "^4.0.1",
    "colors": "^1.4.0",
    "commander": "^5.1.0",
    "cross-fetch": "^3.0.5",
    "form-data": "^3.0.0",
    "fs-extra": "^9.0.1",
    "marked": "^1.1.0",
    "mime-types": "^2.1.27",
    "node-emoji": "^1.10.0",
    "pretty-bytes": "^5.3.0",
    "slugify": "^1.4.2"
  },
  "keywords": [
    "metablock"
  ],
  "bin": {
    "metablock": "dist/metablock.js"
  },
  "devDependencies": {
    "@types/archiver": "^3.1.0",
    "@types/mime-types": "^2.1.0",
    "@types/node-emoji": "^1.8.1",
    "@types/request": "^2.48.5"
  },
  "scripts": {
    "build": "export NODE_ENV=production && tsc",
    "dev": "ts-node src/metablock.ts",
    "watch": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "exec": "tsc",
    "ext": "ts"
  }
}
